{
  "name": "ethereum-bridge-frontend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config config/webpack.config.js --progress --colors --open",
    "build": "node --trace-warnings --trace-deprecation scripts/build.js --stats && cp build/index.html build/404.html",
    "webpack": "webpack --config config/webpack.config.js --display-modules",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:debug": "jest --config=jest.config.debug.js",
    "version": "echo $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/enigmampc/EthereumBridgeFrontend"
  },
  "dependencies": {
    "@metamask/detect-provider": "^1.1.0",
    "@types/react-outside-click-handler": "^1.3.0",
    "async-validator": "1.11.5",
    "bech32": "1.1.4",
    "bignumber.js": "^9.0.1",
    "classnames": "^2.2.6",
    "cogo-toast": "4.2.3",
    "core-decorators": "^0.20.0",
    "grommet": "^2.16.3",
    "lodash": "^4.17.21",
    "mobx": "5.15.0",
    "mobx-react": "6.1.4",
    "mobx-react-router": "4.0.7",
    "moment": "^2.24.0",
    "query-string": "^6.14.0",
    "rc-table": "^7.13.1",
    "rc-tooltip": "^4.2.1",
    "react": "16.12.0",
    "react-copy-to-clipboard": "5.0.2",
    "react-device-detect": "1.15.0",
    "react-dom": "16.12.0",
    "react-in-viewport": "^1.0.0-alpha.16",
    "react-loader-spinner": "3.1.14",
    "react-outside-click-handler": "^1.3.0",
    "react-reel": "^1.2.3",
    "react-reveal": "^1.2.2",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-scrollbars-custom": "^4.0.21",
    "react-select": "^3.2.0",
    "react-switch": "^5.0.1",
    "secretjs": "0.11.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3",
    "style-it": "2.1.4",
    "superagent": "^6.0.0",
    "tslib": "^1.10.0",
    "uuid": "8.3.1",
    "web3": "1.2.11"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.168",
    "@types/react": "16.9.14",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-dom": "16.9.4",
    "@types/react-loader-spinner": "^3.1.3",
    "@types/react-router-dom": "5.1.3",
    "@types/recharts": "^1.8.19",
    "@types/styled-components": "^4.4.0",
    "@types/webpack-dev-server": "3.9.0",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "babel-preset-react-app": "^9.1.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.5",
    "cross-env": "^6.0.3",
    "css-loader": "^4.3.0",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-flowtype": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "^5.0.2",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "fs-extra": "^8.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.1.0",
    "jest": "24.9.0",
    "lint-staged": "^9.5.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "^1.19.1",
    "raf": "^3.4.1",
    "react-dev-utils": "^11.0.2",
    "react-hot-loader": "^4.12.18",
    "resolve": "1.13.1",
    "sass-loader": "^8.0.0",
    "source-map-explorer": "^2.5.1",
    "source-map-loader": "0.2.4",
    "style-loader": "^1.0.1",
    "styled-components": "^4.1.3",
    "stylus": "0.54.8",
    "stylus-loader": "3.0.2",
    "sw-precache-webpack-plugin": "0.11.5",
    "terser-webpack-plugin": "^2.3.4",
    "ts-jest": "^26.5.1",
    "ts-loader": "^6.2.1",
    "ts-node": "8.5.4",
    "tsconfig-paths-webpack-plugin": "3.2.0",
    "typedoc": "^0.16.10",
    "typescript": "4.0.5",
    "url-loader": "3.0.0",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.2",
    "webpack-manifest-plugin": "^2.2.0",
    "whatwg-fetch": "^3.6.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!**/*.d.ts"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/**/*.(spec|test).(j|t)s?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      ".+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      ".+\\.tsx?$": "<rootDir>/config/jest/typescriptTransform.js",
      ".+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|ts|tsx)$"
    ],
    "moduleFileExtensions": [
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "web.js",
      "js",
      "web.jsx",
      "jsx",
      "json",
      "node",
      "mjs"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "./tsconfig.test.json"
      }
    },
    "moduleNameMapper": {
      "src(.*)$": "<rootDir>/src/$1",
      "components(.*)$": "<rootDir>/src/components/$1",
      "constants(.*)$": "<rootDir>/src/constants/$1",
      "contexts(.*)$": "<rootDir>/src/contexts/$1",
      "models(.*)$": "<rootDir>/src/models/$1",
      "modalPages(.*)$": "<rootDir>/src/modalPages/$1",
      "pages(.*)$": "<rootDir>/src/pages/$1",
      "stores(.*)$": "<rootDir>/src/stores/$1",
      "services(.*)$": "<rootDir>/src/services/$1",
      "themes(.*)$": "<rootDir>/src/themes/$1",
      "ui(.*)$": "<rootDir>/src/ui/$1",
      "utils(.*)$": "<rootDir>/src/utils/$1"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "proxy": "http://localhost:3001",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.{css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
