.faqContainer {
  .item {
    margin-bottom 16px;

    .label {
      .labelIcon {
        border-radius 5px
        border 1px solid #212D5E;
        background transparent
        width 21px
        height 20px
        text-align center
        padding 0
        padding-left 1px

        font-family: Nunito;
        font-size: 18px;
        font-weight: 600;
        color: #212D5E;
        line-height 17px
      }

    }

    .textContainer {
      // margin-top -10px
      // margin-left 30px
      padding-right 40px

      ul {
        list-style none
        // padding-left 20px
        list-style-type disc
        margin 10px 0 0 20px

        li {
          margin 2px 0

          ul {
            margin 0 0 0 5px
          }
        }
      }

      .text {
        color #212D5E;
        line-height 25px
        margin 0

        p {
          margin 0
          padding 0
        }

        span {
          color: #47b8eb
        }

        a {
          color #47b8eb
          text-decoration-color #47b8eb
        }
      }
    }
  }

  padding 24px 24px 0px 24px;
}

@media (max-width: 600px) {
  .faqContainer {
    padding 8px 8px 0px 8px !important;
  }

  .item {
    .label {
      .labelText {
        font-size 14px !important;
      }
    }

    .textContainer {
      padding-right 10px !important

      .text {
        font-size 14px !important;
      }

      ul {
        margin 10px 0px !important
      }
    }
  }
}
